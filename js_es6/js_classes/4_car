class Car {
    constructor (make, model, year) {
        this.make = make;
        this.model = model;
        this.year = year;
    }
    carInfo () {
        console.log (`This car is an ${this.make} ${this.model} and it is from the year ${this.year}.`)
    }
}

const car1 = new Car("Audi", "A5", 2000);
car1.carInfo();
