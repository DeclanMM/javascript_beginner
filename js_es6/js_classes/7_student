class Student {
    constructor (name, grades) {
        this.name = name;
        this.grades = grades;
    }
    avgGrade () {
        let x = 0;
        for(let i = 0; i < this.grades.length; i++) {
            x += this.grades[i];
        }
        let avg = x / this.grades.length;
        console.log(avg);
    }
}

const student1 = new Student ("Declan", [50, 45, 60, 68, 42, 50]);
student1.avgGrade();
