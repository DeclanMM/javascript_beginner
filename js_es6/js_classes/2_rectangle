class Rectangle {
    constructor (width, height) {
        this.width = width;
        this.height = height;
    }
    area (width, height) {
        return this.width * this.height;
    }
    perimeter (width, height) {
        return (2 * this.width) + (2 * this.height);
    }
}


const rectangle1 = new Rectangle (10, 15);
console.log(rectangle1.area());
